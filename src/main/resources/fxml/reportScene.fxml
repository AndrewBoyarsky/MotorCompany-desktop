<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox xmlns="http://javafx.com/javafx/8.0.112-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.boyarskycompany.src.controllers.ReportController">
   <children>
      <Button fx:id="agreementReportButton" alignment="BASELINE_LEFT" mnemonicParsing="false" prefHeight="48.0" prefWidth="210.0" text="%agreementReportButton">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Button>
      <Button fx:id="bidReportButton" alignment="BASELINE_LEFT" mnemonicParsing="false" prefHeight="48.0" prefWidth="210.0" text="%bidReportButton">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Button>
      <Button fx:id="transportReportButton" alignment="BASELINE_LEFT" mnemonicParsing="false" prefHeight="48.0" prefWidth="210.0" text="%transportReportButton">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Button>
      <Button fx:id="roadListReportButton" alignment="BASELINE_LEFT" mnemonicParsing="false" prefHeight="48.0" prefWidth="210.0" text="%roadListReportButton">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Button>
      <Button fx:id="waybillReportButton" alignment="BASELINE_LEFT" mnemonicParsing="false" prefHeight="48.0" prefWidth="210.0" text="%waybillReportButton">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Button>
      <Button fx:id="totalBillReportButton" alignment="BOTTOM_LEFT" mnemonicParsing="false" prefHeight="48.0" prefWidth="210.0" text="%totalBillReportButton">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Button>
      <Button fx:id="closeButton" alignment="BASELINE_LEFT" mnemonicParsing="false" prefHeight="25.0" prefWidth="211.0" text="%closeButton">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Button>
   </children>
</VBox>
